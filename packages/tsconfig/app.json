{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Sudoku",
    "extends": "./base.json",
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": false,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "incremental": true,
        "esModuleInterop": true,
        "module": "esnext",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "downlevelIteration": true
    },
    "exclude": [
        "node_modules"
    ]
}